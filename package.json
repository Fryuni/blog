{
  "name": "fryuni-site",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "rm -rf dist .astro && astro build",
    "dev": "astro dev",
    "format": "eslint . --fix",
    "postinstall": "patch-package",
    "lint": "eslint .",
    "prepare": "husky install",
    "preview": "astro preview",
    "start": "astro dev",
    "validate": "astro check && tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,astro}": [
      "eslint --fix"
    ],
    "*.{md,mdx}": [
      "markdownlint --fix"
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.3.1",
    "@astrojs/starlight": "https://gitpkg.now.sh/Fryuni/starlight/packages/starlight?ssr-support",
    "@astrojs/vercel": "^6.0.1",
    "@croct/plug": "^0.13.3",
    "@croct/sdk": "^0.14.0",
    "astro": "^4.0.1",
    "eslint-plugin-import": "^2.29.0",
    "markdownlint-cli": "^0.38.0",
    "squoosh": "^0.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@astrojs/node": "^7.0.0",
    "@croct/eslint-plugin": "^0.7.1",
    "@dword-design/eslint-plugin-import-alias": "^4.0.9",
    "@typescript-eslint/parser": "^6.13.2",
    "astro-eslint-parser": "^0.16.0",
    "eslint": "^8.55.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.30.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "patch-package": "^8.0.0"
  },
  "overrides": {
    "eslint-plugin-import": "$eslint-plugin-import"
  }
}
